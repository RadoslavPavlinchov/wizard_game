{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "camelcase": [
            "warn",
            {
                "properties": "always"
            }
        ],
        "no-const-assign": 2,
        "for-direction": 2
    }
}